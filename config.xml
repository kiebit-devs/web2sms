<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kiebit.web2sms" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>web2sms</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="/app/google-services.json" />
        <resource-file src="release-signing.properties" target="/release-signing.properties" />
        
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-sms-plugin" spec="^1.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <plugin name="havesource-cordova-plugin-push" spec="@havesource/cordova-plugin-push@~1.0.0" />
</widget>
